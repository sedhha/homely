{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@fb-client": ["src/firebase/client/firebase-client.ts"],
      "@fb-server": ["src/firebase/server/firebase-server.ts"],
      "@redux-store/*": ["src/app/*"],
      "@redux-slices/*": ["src/slices/*"],
      "@homely-interfaces/*": ["src/interfaces/*"],
      "@homely-backend/*": ["src/lib/backend/*"],
      "@homely-components/*": ["src/components/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "types.d.ts", "src/components/header.js", "src/components/NavLink.js"],
  "exclude": ["node_modules"]
}
